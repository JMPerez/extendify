{
  "name": "Extendify",
  "short_name": "Extendify",
  "version": "0.1.4",
  "manifest_version": 2,
  "description": "Extendify shows a Spotify Play Button when you browse pages about artists and tracks on Twitter, Wikipedia, Youtube and many more.",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js" : [
        "js/jquery.min.js",
        "js/search.js",
        "js/lib.js",
        "js/providers/allmusic.js",
        "js/providers/amazon.js",
        "js/providers/ebay.js",
        "js/providers/gmail.js",
        "js/providers/google.js",
        "js/providers/piratebay.js",
        "js/providers/twitter.js",
        "js/providers/wikipedia.js",
        "js/providers/youtube.js",
        "js/background.js"
        ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
